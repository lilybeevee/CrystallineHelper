<?xml version="1.0" encoding="utf-8" ?>
<Sprites>
  <keyberry path="collectables/keyberry/" start="idle">
    <Center/>
    <Loop id="idle" path="normal" delay=".1" frames="0-6,0-6,0-6,0-6,0-7" />
    <Loop id="noFlash" path="normal" delay=".1" frames="0-6"/>
    <Anim id="collect" path="normal" delay=".07" frames="7-11"/>
    <Loop id="flap" path="wings" delay=".08" frames="0-8,0-8,0-7,9" />
	  
    <Anim id="fade" path="normal" delay=".08" frames="12-14,14*10,15-19"/>
	  <Anim id="fade1up" path="normal" delay=".07" frames="20,21,22,21,22,21,22,21,22,21,22,21,22-27"/>
  </keyberry>
  <keyberrySeed path="collectables/keyberry/" start="idle">
    <Center/>
    <Loop id="idle" path="seed" delay="0.1" frames="0*19,1" />
    <Loop id="noFlash" path="seed" frames="0" />
  </keyberrySeed>
  
  <locked_intro_car_body path="objects/lockedIntroCar/" start="idle">
    <Center/>
    <Loop id="idle" path="idle" delay="0.1" frames="0"/>
    <Anim id="open" path="open" delay="0.06" frames="0-6"/>
    <Loop id="openIdle" path="open" delay="0.1" frames="7-10"/>
    <Anim id="burst" path="burst" delay="0.06" frames="0-10"/>
  </locked_intro_car_body>
  
  <locked_intro_car_wheels path="objects/lockedIntroCar/wheels_" start="idle">
    <Center/>
    <Loop id="idle" path="idle" delay="0.1" frames="0"/>
    <Anim id="burst" path="burst" delay="0.06" frames="0,1*2,2-3,4*6"/>
  </locked_intro_car_wheels>
  
  <boostBumper path="objects/boostBumper/" start="idle">
    <Center/>
    <Loop id="idle" path="booster" delay="0.1" frames="0-4,0-4,0-9"/>
    <Loop id="inside" path="booster" delay="0.1" frames="10-13"/>
    <Anim id="burst" path="booster" delay="0.06" frames="14-22"/>
  </boostBumper>
  
  <kaizoblock path="objects/kaizoBlock/kaizoblock" start="idle">
    <Center/>
    <Loop id="idle" path="" delay="0.2" frames="0-2,1"/>
    <Anim id="flash" path="" delay="0.05" frames="3-6,7*3,8*2" goto="idle"/>
  </kaizoblock>

  <fastBumpBlockCenterFire path="objects/BumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="fire_center" frames="2-6" delay="0.08"/>
  </fastBumpBlockCenterFire>
  <fastBumpBlockCenterIce path="objects/BumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="ice_center" frames="0,2,4-6" delay="0.1"/>
  </fastBumpBlockCenterIce>
  <dashSwitch_deadly path="objects/deadlyDashButton/dashButton" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="" delay="0.06" frames="0-20"/>
    <Loop id="pushed" path="" delay="0.08" frames="27"/>
    <Anim id="push" path="" delay="0.07" frames="21-27" goto="pushed"/>
  </dashSwitch_deadly>
  <energyBooster path="objects/energyBooster/" start="loop">
    <Justify x="0.5" y="0.5"/>
    <Loop id="loop" path="booster" delay="0.1" frames="0-4"/>
    <Loop id="inside" path="booster" delay="0.1" frames="5-8"/>
    <Loop id="spin" path="booster" delay="0.06" frames="18-25"/>
    <Anim id="pop" path="booster" delay="0.08" frames="9-17"/>
  </energyBooster>
  <energyBoosterRedirect path="objects/energyBoosterRedirect/" start="loop">
    <Justify x="0.5" y="0.5"/>
    <Loop id="loop" path="booster" delay="0.1" frames="0-4"/>
    <Loop id="inside" path="booster" delay="0.1" frames="5-8"/>
    <Loop id="spin" path="booster" delay="0.06" frames="18-25"/>
    <Anim id="pop" path="booster" delay="0.08" frames="9-17"/>
  </energyBoosterRedirect>
  <bumperBlock_face path="objects/bumperBlock/" start="idle">
    <Center/>
    <Loop id="idle" path="idle_face" delay="0.08"/>
    <Anim id="hit" path="hit" delay="0.08" goto="idle"/>
  </bumperBlock_face>
  <flccSmileyPuffer path="objects/flccSmileyPuffer/" start="idle">
    <Center/>
    <Loop id="idle" path="idle" delay="0.08"/>
    <Anim id="alert" path="alert" goto="alerted" delay="0.08"/>
    <Loop id="alerted" path="alerted" delay="0.08"/>
    <Anim id="explode" path="explode" goto="hidden" delay="0.08"/>
    <Anim id="unalert" path="alert" goto="idle" delay="0.08" frames="1,0"/>
    <Loop id="hidden" path="hidden" delay="0.08"/>
    <Anim id="recover" path="recover" goto="idle" delay="0.05"/>
  </flccSmileyPuffer>
</Sprites>